import java.util.ArrayList;
import java.util.Collection;
import java.util.Collections;
public class array_list {

	public static void main(String[] args) {
	
		ArrayList obj=new ArrayList();
		obj.add(1);
		obj.add(2);
		obj.add(3);
		System.out.println(obj);
		obj.remove(2);
		System.out.println(obj);
	
	 System.out.println( obj.get(0));
	 obj.set(0, 10);
	 System.out.println(obj);
	 System.out.println(obj.contains(10));
	
	 System.out.println( obj.remove(0));
	 System.out.println(obj);
	 Collections.addAll(obj, 5,6,7,8);
	 System.out.println("the element of arraylist1"+obj);
	 
ArrayList obj2=new ArrayList();
Collections.addAll(obj2, 2,5,6);
System.out.println("the element of array list2"+obj2);
obj.removeAll(obj2);
System.out.println(obj);
	}

}
